<ul>
  <% comments.each do |comment| %>
    <% if comment.id.present? %>
      <li>
        <p><%= comment.content %></p>
        <%= link_to "delete", [comment.course, comment], method: :delete%>
        <%= link_to "edit ", edit_course_comment_path(comment.course, comment) %></p>
      </li>
    <% end %>
  <% end %>
</ul>


